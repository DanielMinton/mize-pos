{
  "name": "mise-pos",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "db:push": "pnpm --filter @mise-pos/database db:push",
    "db:generate": "pnpm --filter @mise-pos/database db:generate",
    "db:studio": "pnpm --filter @mise-pos/database db:studio",
    "db:seed": "pnpm --filter @mise-pos/database db:seed",
    "db:migrate": "pnpm --filter @mise-pos/database db:migrate",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.6.3"
  }
}
